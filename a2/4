using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите трехзначное число ");
            int numb = int.Parse(Console.ReadLine());
            Console.WriteLine("Зачеркнутая первая слева цифра и приписана в конце " + (numb / 10 % 10) + numb % 10 + numb / 100);
            Console.ReadLine();

        }
    }
}
